def averageOfList(num):
    sumOfNumbers = 0
    for t in num:
        sumOfNumbers = sumOfNumbers + t

    avg = sumOfNumbers / len(num)
    return avg

print("The average of List is", averageOfList([19, 21, 46, 11, 18]))


def reverseNum(num1):
    print(int(str(num1)[::-1]))
reverseNum(123)

num2=123467
def findSumOfDig(num2):
    s=sum(int(digit) for digit in str(num2))
    return s
print(findSumOfDig(num2))
